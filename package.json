{
  "name": "ajme-v3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/data/payload.config.ts nodemon",
    "types": "cd src/data && payload generate:types",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/data/payload.config.ts payload build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "cross-env PAYLOAD_CONFIG_PATH=dist/data/payload.config.js next build",
    "build": "cross-env NODE_ENV=production npm run build:payload && npm run build:server && npm run copyfiles && npm run build:next",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/data/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "lint": "next lint",
    "test": "vitest"
  },
  "dependencies": {
    "@ark-ui/react": "^2.2.3",
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/lib-storage": "^3.525.0",
    "@payloadcms/bundler-vite": "^0.1.6",
    "@payloadcms/bundler-webpack": "^1.0.6",
    "@payloadcms/db-mongodb": "^1.4.3",
    "@payloadcms/plugin-cloud-storage": "^1.1.2",
    "@payloadcms/richtext-lexical": "^0.7.0",
    "@picocss/pico": "^2.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@types/express": "^4.17.21",
    "aws-crt": "^1.21.1",
    "bright": "^0.8.4",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "focus-trap-react": "^10.2.3",
    "next": "14.1.0",
    "open-color": "^1.9.1",
    "payload": "^2.11.2",
    "react": "^18",
    "react-dom": "^18",
    "react-responsive": "^9.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.2.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "nodemon": "^3.1.0",
    "sass": "^1.71.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^1.3.1"
  }
}
